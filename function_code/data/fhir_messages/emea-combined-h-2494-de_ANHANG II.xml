<section>
			<title value="·                Risikomanagement-Plan (RMP)"/>
				<code>
					<coding>
						<system value="http://spor.ema.europa.eu/v1/example-sections"/>
						<code value="${document[n].section[n].subsection[n].typeCode}"/>
